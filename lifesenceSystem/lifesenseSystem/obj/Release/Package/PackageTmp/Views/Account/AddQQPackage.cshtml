@{
    ViewBag.Title = "新增QQ包";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        新增QQ包
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="../Account/Index"><i class="fa fa-dashboard"></i>首页</a></li>
        <li><a href="#">会员QQ推广</a></li>
        <li class="active">新增QQ包</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-6">
            <div class="box">
                <div class="box-header">
                    <!--<h3 class="box-title">Data Table With Full Features</h3>-->
                </div><!-- /.box-header -->
                <!-- form start -->
                <form id="form1" class="form-horizontal" method="post">
                    <div class="box-body">
                        <div class="form-group">
                            <label class="col-sm-3 control-label" for="QQPName">QQ包文件名<span style="color:red">*</span></label>
                            <div class="col-sm-8">
                                <input type="text" name="QQPName" id="QQPName" class="form-control" readonly="readonly" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label" for="exampleInputFile">QQ包上传</label>
                            <div class="col-sm-8">
                                <input id="exampleInputFile" name="exampleInputFile" type="file" />
                            </div>
                        </div>
                    </div><!-- /.box-body -->
                    <div class="box-footer">
                        <button type="submit" class="btn btn-primary">保存</button>
                    </div>
                </form>
            </div><!-- /.box -->
        </div><!--/.col (left) -->
    </div><!-- /.row -->
</section><!-- /.content -->
