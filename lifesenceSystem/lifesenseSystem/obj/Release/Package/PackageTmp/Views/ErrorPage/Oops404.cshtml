@{
    ViewBag.Title = "404 错误";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        404 错误 页面
    </h1>
    <ol class="breadcrumb">
        @{
            var directory = "";
            if (ViewData["usertype"].ToString() == "管理员")
            {
                directory = "Admin";
            }
            else
            {
                directory = "Account";
            }
        }
        <li><a href="../@directory/Index"><i class="fa fa-dashboard"></i>首页</a></li>
        <li class="active">404 错误</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="error-page">
        <h2 class="headline text-yellow">404</h2>
        <div class="error-content">
            <h3><i class="fa fa-warning text-yellow"></i>哎呦! 页面找不到</h3>
            <p>
                我们找不到你要找的那一页。您浏览的页面可能被删除，转移或暂时不可用
                遇到此情况的概率同中彩票差不多哦！
                买张彩票试试运气吧！
                同时，您可以返回到<a href="../@directory/Index">首页</a>
            </p>
            <form class="search-form">
               <!-- <div class="input-group">
                    <input type="text" name="search" class="form-control" placeholder="Search" />
                    <div class="input-group-btn">
                        <button type="submit" name="submit" class="btn btn-warning btn-flat"><i class="fa fa-search"></i></button>
                    </div>
                </div> /.input-group -->
            </form>
        </div><!-- /.error-content -->
    </div><!-- /.error-page -->
</section><!-- /.content -->
